            <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import org.jsoup.Jsoup
import org.jsoup.nodes.Document
import org.jsoup.select.Elements
import org.apache.jmeter.threads.JMeterContext.TestLogicalAction
import groovy.json.JsonSlurper
import org.apache.jmeter.assertions.AssertionResult

def should_restart = false
def expectedAuthor = vars.get(&quot;SEARCH_VALUE&quot;).trim()
def response = prev.getResponseDataAsString()

Document doc = Jsoup.parse(response)
Elements elements = doc.select(&quot;table tbody tr td[class=&apos;author&apos;] a&quot;)

try {
    if(!(&quot;200&quot;.equals(prev.getResponseCode())) || elements.isEmpty()) {
        should_restart = true
    } else {
        def actualAuthors = elements.collect { it.text().trim() }
        if (!actualAuthors.contains(expectedAuthor)) {
            should_restart = true
            AssertionResult.setFailureMessage(&quot;Expected author &apos;${expectedAuthor}&apos; not found in the response.&quot;)
            AssertionResult.setFailure(true)
        } else {
			log.info &quot;Founded Authors: $actualAuthors&quot;
        	}
    }
} catch (Exception ex) {
    should_restart = true
    AssertionResult.setFailureMessage(&quot;An error occurred: ${ex.message}&quot;)
    AssertionResult.setFailure(true)
}

if(should_restart) {
    ctx.setTestLogicalAction(TestLogicalAction.START_NEXT_ITERATION_OF_CURRENT_LOOP)
}
</stringProp>
